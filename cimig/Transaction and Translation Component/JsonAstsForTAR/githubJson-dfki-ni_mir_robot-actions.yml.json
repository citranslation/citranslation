{
 "children": [
  {
   "children": [{
    "children": [],
    "type": "build and run ros tests"
   }],
   "type": "name"
  },
  {
   "children": [
    {
     "children": [],
     "type": "push"
    },
    {
     "children": [],
     "type": "pull_request"
    }
   ],
   "type": "on"
  },
  {
   "children": [{
    "children": [
     {
      "children": [{
       "children": [{
        "children": [{
         "children": [],
         "type": "noetic"
        }],
        "type": "rosdistro"
       }],
       "type": "matrix"
      }],
      "type": "strategy"
     },
     {
      "children": [{
       "children": [],
       "type": "ubuntu-latest"
      }],
      "type": "runs-on"
     },
     {
      "children": [{
       "children": [{
        "children": [],
        "type": "ros:${{ matrix.rosdistro }}-ros-core"
       }],
       "type": "image"
      }],
      "type": "container"
     },
     {
      "children": [
       {
        "children": [
         {
          "children": [{
           "children": [],
           "type": "install apt dependencies"
          }],
          "type": "name"
         },
         {
          "children": [{
           "children": [],
           "type": "sudo apt-cmd"
          }],
          "type": "run"
         }
        ],
        "type": "OBJ-NODE"
       },
       {
        "children": [
         {
          "children": [{
           "children": [],
           "type": "install pip dependencies"
          }],
          "type": "name"
         },
         {
          "children": [{
           "children": [],
           "type": "pip install pre-commit"
          }],
          "type": "run"
         }
        ],
        "type": "OBJ-NODE"
       },
       {
        "children": [
         {
          "children": [{
           "children": [],
           "type": "checkout repository"
          }],
          "type": "name"
         },
         {
          "children": [{
           "children": [],
           "type": "actions/checkout@version"
          }],
          "type": "uses"
         },
         {
          "children": [{
           "children": [{
            "children": [],
            "type": "src/mir_robot"
           }],
           "type": "path"
          }],
          "type": "with"
         }
        ],
        "type": "OBJ-NODE"
       },
       {
        "children": [
         {
          "children": [{
           "children": [],
           "type": "use rosdep to install remaining dependencies"
          }],
          "type": "name"
         },
         {
          "children": [{
           "children": [],
           "type": "sudo rosdep init\\nrosdep update\\nrosdep install --from-paths src -i -y --rosdistro ${{ matrix.rosdistro }}\\n"
          }],
          "type": "run"
         }
        ],
        "type": "OBJ-NODE"
       },
       {
        "children": [
         {
          "children": [{
           "children": [],
           "type": "build"
          }],
          "type": "name"
         },
         {
          "children": [{
           "children": [],
           "type": ". /opt/ros/${{ matrix.rosdistro }}/PATH-OR-URL\\ncatkin init\\ncatkin config -j 1 -p 1\\ncatkin build --limit-status-rate 0.1 --no-notify\\ncatkin build --limit-status-rate 0.1 --no-notify --make-args tests\\n"
          }],
          "type": "run"
         }
        ],
        "type": "OBJ-NODE"
       },
       {
        "children": [
         {
          "children": [{
           "children": [],
           "type": "run tests"
          }],
          "type": "name"
         },
         {
          "children": [{
           "children": [],
           "type": ". devel/PATH-OR-URL\\ncatkin run_tests\\ncatkin_test_results\\n"
          }],
          "type": "run"
         }
        ],
        "type": "OBJ-NODE"
       },
       {
        "children": [
         {
          "children": [{
           "children": [],
           "type": "run pre-commit hooks"
          }],
          "type": "name"
         },
         {
          "children": [{
           "children": [],
           "type": "cd-cmd"
          }],
          "type": "run"
         }
        ],
        "type": "OBJ-NODE"
       }
      ],
      "type": "steps"
     }
    ],
    "type": "build"
   }],
   "type": "build"
  }
 ],
 "type": "github-file"
}