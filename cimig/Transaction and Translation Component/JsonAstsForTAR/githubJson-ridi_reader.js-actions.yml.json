{
 "children": [
  {
   "children": [{
    "children": [],
    "type": "reader.js ci (lint & test)"
   }],
   "type": "name"
  },
  {
   "children": [{
    "children": [{
     "children": [
      {
       "children": [],
       "type": ".github/workflows/check.yml"
      },
      {
       "children": [],
       "type": "src/**"
      }
     ],
     "type": "paths"
    }],
    "type": "push"
   }],
   "type": "on"
  },
  {
   "children": [{
    "children": [{
     "children": [],
     "type": "18"
    }],
    "type": "node_version"
   }],
   "type": "env"
  },
  {
   "children": [{
    "children": [
     {
      "children": [{
       "children": [],
       "type": "run lint"
      }],
      "type": "name"
     },
     {
      "children": [{
       "children": [],
       "type": "ubuntu-latest"
      }],
      "type": "runs-on"
     },
     {
      "children": [
       {
        "children": [
         {
          "children": [{
           "children": [],
           "type": "checkout"
          }],
          "type": "name"
         },
         {
          "children": [{
           "children": [],
           "type": "actions/checkout@version"
          }],
          "type": "uses"
         }
        ],
        "type": "OBJ-NODE"
       },
       {
        "children": [
         {
          "children": [{
           "children": [],
           "type": "set up node ${{ env.node_version }}"
          }],
          "type": "name"
         },
         {
          "children": [{
           "children": [],
           "type": "actions/setup-node@version"
          }],
          "type": "uses"
         },
         {
          "children": [{
           "children": [{
            "children": [],
            "type": "${{ env.node_version }}"
           }],
           "type": "node-version"
          }],
          "type": "with"
         }
        ],
        "type": "OBJ-NODE"
       },
       {
        "children": [
         {
          "children": [{
           "children": [],
           "type": "set up node_modules cache"
          }],
          "type": "name"
         },
         {
          "children": [{
           "children": [],
           "type": "actions/cache@version"
          }],
          "type": "uses"
         },
         {
          "children": [{
           "children": [],
           "type": "node-cache"
          }],
          "type": "id"
         },
         {
          "children": [
           {
            "children": [{
             "children": [],
             "type": "node_modules"
            }],
            "type": "path"
           },
           {
            "children": [{
             "children": [],
             "type": "${{ runner.os }}-node-modules-${{ secrets.cache_version }}-${{ hashfiles(arg) }} }}"
            }],
            "type": "key"
           }
          ],
          "type": "with"
         }
        ],
        "type": "OBJ-NODE"
       },
       {
        "children": [
         {
          "children": [{
           "children": [],
           "type": "install node packages"
          }],
          "type": "name"
         },
         {
          "children": [{
           "children": [],
           "type": "steps.node-cache.outputs.cache-hit != 'true'"
          }],
          "type": "if"
         },
         {
          "children": [{
           "children": [],
           "type": "yarn install --frozen-lockfile"
          }],
          "type": "run"
         }
        ],
        "type": "OBJ-NODE"
       },
       {
        "children": [
         {
          "children": [{
           "children": [],
           "type": "run lint"
          }],
          "type": "name"
         },
         {
          "children": [{
           "children": [],
           "type": "yarn lint"
          }],
          "type": "run"
         }
        ],
        "type": "OBJ-NODE"
       }
      ],
      "type": "steps"
     }
    ],
    "type": "run-lint"
   }],
   "type": "build"
  }
 ],
 "type": "github-file"
}