          }
         ],
         "type": "OBJ-NODE"
        },
        {
         "children": [
          {
           "children": [{
            "children": [],
            "type": "install postgresql, postgis and gdal"
           }],
           "type": "name"
          },
          {
           "children": [{
            "children": [],
            "type": "sudo apt-cmd"
           }],
           "type": "run"
          }
         ],
         "type": "OBJ-NODE"
        },
        {
         "children": [
          {
           "children": [{
            "children": [],
            "type": "change postgresql port to 5432"
           }],
           "type": "name"
          },
          {
           "children": [{
            "children": [],
            "type": "sudo sed -i 's/^port = .*/port = 5432/' /etc/postgresql/10/main/postgresql.conf\\n"
           }],
           "type": "run"
          }
         ],
         "type": "OBJ-NODE"
        },
        {
         "children": [
          {
           "children": [{
            "children": [],
            "type": "start postgresql server"
           }],
           "type": "name"
          },
          {
           "children": [{
            "children": [],
            "type": "sudo systemctl start postgresql@10-main"
           }],
           "type": "run"
          }
         ],
         "type": "OBJ-NODE"
        },
        {
         "children": [
          {
           "children": [{
            "children": [],
            "type": "allow runner user to create postgresql databases"
           }],
           "type": "name"
          },
          {
           "children": [{
            "children": [],
            "type": "sudo -u postgres createuser --createdb runner"
           }],
           "type": "run"
          }
         ],
         "type": "OBJ-NODE"
        },
        {
         "children": [
          {
           "children": [{
            "children": [],
            "type": "create postgis extension"
           }],
           "type": "name"
          },
          {
           "children": [{
            "children": [],
            "type": "sudo -u postgres psql -u postgres template1 -c \\\"create extension postgis\\\"\\n"
           }],
           "type": "run"
          }
         ],
         "type": "OBJ-NODE"
        },
        {
         "children": [
          {
           "children": [{
            "children": [],
            "type": "install tox and tox-gh-actions"
           }],
           "type": "name"
          },
          {
           "children": [{
            "children": [],
            "type": "pip install tox tox-gh-actions"
           }],
           "type": "run"
          }
         ],
         "type": "OBJ-NODE"
        },
