{
 "children": [
  {
   "children": [{
    "children": [],
    "type": "test runner"
   }],
   "type": "name"
  },
  {
   "children": [
    {
     "children": [{
      "children": [{
       "children": [],
       "type": "branch-name"
      }],
      "type": "branches"
     }],
     "type": "push"
    },
    {
     "children": [{
      "children": [{
       "children": [],
       "type": "branch-name"
      }],
      "type": "branches"
     }],
     "type": "pull_request"
    }
   ],
   "type": "on"
  },
  {
   "children": [{
    "children": [
     {
      "children": [{
       "children": [],
       "type": "ubuntu-latest"
      }],
      "type": "runs-on"
     },
     {
      "children": [
       {
        "children": [
         {
          "children": [{
           "children": [],
           "type": "actions/checkout@version"
          }],
          "type": "uses"
         },
         {
          "children": [{
           "children": [{
            "children": [],
            "type": "0"
           }],
           "type": "fetch-depth"
          }],
          "type": "with"
         }
        ],
        "type": "OBJ-NODE"
       },
       {
        "children": [{
         "children": [{
          "children": [],
          "type": "npm-cmd"
         }],
         "type": "run"
        }],
        "type": "OBJ-NODE"
       },
       {
        "children": [{
         "children": [{
          "children": [],
          "type": "npm-cmd"
         }],
         "type": "run"
        }],
        "type": "OBJ-NODE"
       },
       {
        "children": [
         {
          "children": [{
           "children": [],
           "type": "validate current commit (last commit) with commitlint"
          }],
          "type": "name"
         },
         {
          "children": [{
           "children": [],
           "type": "github.event_name == 'push'"
          }],
          "type": "if"
         },
         {
          "children": [{
           "children": [],
           "type": "npx commitlint --last --verbose"
          }],
          "type": "run"
         }
        ],
        "type": "OBJ-NODE"
       },
       {
        "children": [
         {
          "children": [{
           "children": [],
           "type": "validate pr commits with commitlint"
          }],
          "type": "name"
         },
         {
          "children": [{
           "children": [],
           "type": "github.event_name == 'pull_request'"
          }],
          "type": "if"
         },
         {
          "children": [{
           "children": [],
           "type": "npx commitlint --from ${{ github.event.pull_request.base.sha }} --to ${{ github.event.pull_request.head.sha }} --verbose"
          }],
          "type": "run"
         }
        ],
        "type": "OBJ-NODE"
       },
       {
        "children": [{
         "children": [{
          "children": [],
          "type": "npm-cmd"
         }],
         "type": "run"
        }],
        "type": "OBJ-NODE"
       },
       {
        "children": [
         {
          "children": [{
           "children": [],
           "type": "upload code coverage"
          }],
          "type": "name"
         },
         {
          "children": [{
           "children": [],
           "type": "codecov/codecov-action@version"
          }],
          "type": "uses"
         },
         {
          "children": [
           {
            "children": [{
             "children": [],
             "type": "TRUE"
            }],
            "type": "fail_ci_if_error"
           },
           {
            "children": [{
             "children": [],
             "type": "./coverage/coverage-final.json,./coverage-jest/coverage-final.json,./coverage-cypress/coverage-final.json"
            }],
            "type": "files"
           },
           {
            "children": [{
             "children": [],
             "type": "${{ secrets.codecov_token }}"
            }],
            "type": "token"
           }
          ],
          "type": "with"
         }
        ],
        "type": "OBJ-NODE"
       },
       {
        "children": [{
         "children": [{
          "children": [],
          "type": "npm-cmd"
         }],
         "type": "run"
        }],
        "type": "OBJ-NODE"
       }
      ],
      "type": "steps"
     }
    ],
    "type": "test"
   }],
   "type": "build"
  }
 ],
 "type": "github-file"
}