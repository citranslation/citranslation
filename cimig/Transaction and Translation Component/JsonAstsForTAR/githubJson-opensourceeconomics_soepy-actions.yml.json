{
 "children": [
  {
   "children": [{
    "children": [],
    "type": "continuous integration workflow"
   }],
   "type": "name"
  },
  {
   "children": [
    {
     "children": [{
      "children": [{
       "children": [],
       "type": "branch-name"
      }],
      "type": "branches"
     }],
     "type": "push"
    },
    {
     "children": [{
      "children": [{
       "children": [],
       "type": "branch-name"
      }],
      "type": "branches"
     }],
     "type": "pull_request"
    },
    {
     "children": [],
     "type": "workflow_dispatch"
    }
   ],
   "type": "on"
  },
  {
   "children": [{
    "children": [
     {
      "children": [{
       "children": [],
       "type": "run tests for-cmd"
      }],
      "type": "name"
     },
     {
      "children": [{
       "children": [],
       "type": "${{ matrix.os }}"
      }],
      "type": "runs-on"
     },
     {
      "children": [
       {
        "children": [{
         "children": [],
         "type": "FALSE"
        }],
        "type": "fail-fast"
       },
       {
        "children": [
         {
          "children": [{
           "children": [],
           "type": "ubuntu-latest"
          }],
          "type": "os"
         },
         {
          "children": [{
           "children": [],
           "type": "3.8"
          }],
          "type": "python-version"
         }
        ],
        "type": "matrix"
       }
      ],
      "type": "strategy"
     },
     {
      "children": [
       {
        "children": [{
         "children": [{
          "children": [],
          "type": "actions/checkout@version"
         }],
         "type": "uses"
        }],
        "type": "OBJ-NODE"
       },
       {
        "children": [
         {
          "children": [{
           "children": [],
           "type": "conda-incubator/setup-miniconda@version"
          }],
          "type": "uses"
         },
         {
          "children": [
           {
            "children": [{
             "children": [],
             "type": "soepy"
            }],
            "type": "activate-environment"
           },
           {
            "children": [{
             "children": [],
             "type": "TRUE"
            }],
            "type": "auto-update-conda"
           },
           {
            "children": [{
             "children": [],
             "type": "environment.yml"
            }],
            "type": "environment-file"
           },
           {
            "children": [{
             "children": [],
             "type": "${{ matrix.python-version }}"
            }],
            "type": "python-version"
           }
          ],
          "type": "with"
         }
        ],
        "type": "OBJ-NODE"
       },
       {
        "children": [
         {
          "children": [{
           "children": [],
           "type": "run pytest."
          }],
          "type": "name"
         },
         {
          "children": [{
           "children": [],
           "type": "bash -l {0}"
          }],
          "type": "shell"
         },
         {
          "children": [{
           "children": [],
           "type": "pytest --cov-report=xml --cov=./"
          }],
          "type": "run"
         }
        ],
        "type": "OBJ-NODE"
       },
       {
        "children": [
         {
          "children": [{
           "children": [],
           "type": "upload coverage-cmd"
          }],
          "type": "name"
         },
         {
          "children": [{
           "children": [],
           "type": "codecov/codecov-action@version"
          }],
          "type": "uses"
         },
         {
          "children": [{
           "children": [{
            "children": [],
            "type": "${{ secrets.codecov_token }}"
           }],
           "type": "token"
          }],
          "type": "with"
         }
        ],
        "type": "OBJ-NODE"
       },
       {
        "children": [
         {
          "children": [{
           "children": [],
           "type": "run pre-commits."
          }],
          "type": "name"
         },
         {
          "children": [{
           "children": [],
           "type": "bash -l {0}"
          }],
          "type": "shell"
         },
         {
          "children": [{
           "children": [],
           "type": "pre-commit install -f --install-hooks\\npre-commit run --all-files"
          }],
          "type": "run"
         }
        ],
        "type": "OBJ-NODE"
       }
      ],
      "type": "steps"
     }
    ],
    "type": "run-tests"
   }],
   "type": "build"
  }
 ],
 "type": "github-file"
}