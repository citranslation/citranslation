{
 "children": [
  {
   "children": [{
    "children": [],
    "type": "build and deploy a branch"
   }],
   "type": "name"
  },
  {
   "children": [
    {
     "children": [
      {
       "children": [{
        "children": [],
        "type": "branch-name"
       }],
       "type": "branches"
      },
      {
       "children": [{
        "children": [],
        "type": "*"
       }],
       "type": "tags"
      }
     ],
     "type": "push"
    },
    {
     "children": [],
     "type": "workflow_dispatch"
    }
   ],
   "type": "on"
  },
  {
   "children": [
    {
     "children": [{
      "children": [],
      "type": "TRUE"
     }],
     "type": "ci"
    },
    {
     "children": [{
      "children": [],
      "type": "protocol-library-builds"
     }],
     "type": "deploy_bucket"
    }
   ],
   "type": "env"
  },
  {
   "children": [{
    "children": [
     {
      "children": [{
       "children": [],
       "type": "ubuntu-latest"
      }],
      "type": "runs-on"
     },
     {
      "children": [{
       "children": [],
       "type": "10"
      }],
      "type": "timeout-minutes"
     },
     {
      "children": [
       {
        "children": [
         {
          "children": [{
           "children": [],
           "type": "actions/setup-python@version"
          }],
          "type": "uses"
         },
         {
          "children": [{
           "children": [{
            "children": [],
            "type": "3.8"
           }],
           "type": "python-version"
          }],
          "type": "with"
         }
        ],
        "type": "OBJ-NODE"
       },
       {
        "children": [{
         "children": [{
          "children": [],
          "type": "curl-cmd"
         }],
         "type": "run"
        }],
        "type": "OBJ-NODE"
       },
       {
        "children": [{
         "children": [{
          "children": [],
          "type": "sudo apt-cmd"
         }],
         "type": "run"
        }],
        "type": "OBJ-NODE"
       },
       {
        "children": [{
         "children": [{
          "children": [],
          "type": "actions/checkout@version"
         }],
         "type": "uses"
        }],
        "type": "OBJ-NODE"
       },
       {
        "children": [{
         "children": [{
          "children": [],
          "type": "pip install -e otcustomizers\\npip install -r protolib/requirements.txt\\npip install flake8==3.8.4 pytest\\n"
         }],
         "type": "run"
        }],
        "type": "OBJ-NODE"
       },
       {
        "children": [{
         "children": [{
          "children": [],
          "type": "make setup"
         }],
         "type": "run"
        }],
        "type": "OBJ-NODE"
       },
       {
        "children": [{
         "children": [{
          "children": [],
          "type": "python-cmd"
         }],
         "type": "run"
        }],
        "type": "OBJ-NODE"
       },
       {
        "children": [
         {
          "children": [{
           "children": [],
           "type": "lint with flake8"
          }],
          "type": "name"
         },
         {
          "children": [{
           "children": [],
           "type": "flake8 protocols/ protolib/\\n"
          }],
          "type": "run"
         }
        ],
        "type": "OBJ-NODE"
       },
       {
        "children": [
         {
          "children": [{
           "children": [],
           "type": "check builds up to date"
          }],
          "type": "name"
         },
         {
          "children": [{
           "children": [],
           "type": "./PATH-OR-URL\\n"
          }],
          "type": "run"
         }
        ],
        "type": "OBJ-NODE"
       },
       {
        "children": [
         {
          "children": [{
           "children": [],
           "type": "check PATH-OR-URL"
          }],
          "type": "name"
         },
         {
          "children": [{
           "children": [],
           "type": "python-cmd"
          }],
          "type": "run"
         }
        ],
        "type": "OBJ-NODE"
       },
       {
        "children": [
         {
          "children": [{
           "children": [],
           "type": "build"
          }],
          "type": "name"
         },
         {
          "children": [{
           "children": [],
           "type": "make build\\n"
          }],
          "type": "run"
         }
        ],
        "type": "OBJ-NODE"
       },
       {
        "children": [
         {
          "children": [{
           "children": [],
           "type": "deploy"
          }],
          "type": "name"
         },
         {
          "children": [
           {
            "children": [{
             "children": [],
             "type": "${{ secrets.aws_access_key_id }}"
            }],
            "type": "aws_access_key_id"
           },
           {
            "children": [{
             "children": [],
             "type": "${{ secrets.aws_secret_access_key }}"
            }],
            "type": "aws_secret_access_key"
           },
           {
            "children": [{
             "children": [],
             "type": "us-west-2"
            }],
            "type": "aws_default_region"
           }
          ],
          "type": "env"
         },
         {
          "children": [{
           "children": [],
           "type": "aws s3 sync --acl=private releases/deploy/ s3://${{env.deploy_bucket}}/${{github.ref_name}}"
          }],
          "type": "run"
         }
        ],
        "type": "OBJ-NODE"
       }
      ],
      "type": "steps"
     }
    ],
    "type": "build-deploy"
   }],
   "type": "build"
  }
 ],
 "type": "github-file"
}