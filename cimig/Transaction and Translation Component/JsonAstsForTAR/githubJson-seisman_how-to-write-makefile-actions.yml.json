{
 "children": [
  {
   "children": [{
    "children": [],
    "type": "deploy"
   }],
   "type": "name"
  },
  {
   "children": [
    {
     "children": [],
     "type": "pull_request"
    },
    {
     "children": [{
      "children": [{
       "children": [],
       "type": "branch-name"
      }],
      "type": "branches"
     }],
     "type": "push"
    }
   ],
   "type": "on"
  },
  {
   "children": [{
    "children": [
     {
      "children": [{
       "children": [],
       "type": "ubuntu-latest"
      }],
      "type": "runs-on"
     },
     {
      "children": [{
       "children": [{
        "children": [{
         "children": [],
         "type": "bash -l {0}"
        }],
        "type": "shell"
       }],
       "type": "run"
      }],
      "type": "defaults"
     },
     {
      "children": [
       {
        "children": [
         {
          "children": [{
           "children": [],
           "type": "cancel previous runs"
          }],
          "type": "name"
         },
         {
          "children": [{
           "children": [],
           "type": "styfle/cancel-workflow-action@version"
          }],
          "type": "uses"
         }
        ],
        "type": "OBJ-NODE"
       },
       {
        "children": [
         {
          "children": [{
           "children": [],
           "type": "checkout"
          }],
          "type": "name"
         },
         {
          "children": [{
           "children": [],
           "type": "actions/checkout@version"
          }],
          "type": "uses"
         }
        ],
        "type": "OBJ-NODE"
       },
       {
        "children": [
         {
          "children": [{
           "children": [],
           "type": "set up python"
          }],
          "type": "name"
         },
         {
          "children": [{
           "children": [],
           "type": "actions/setup-python@version"
          }],
          "type": "uses"
         },
         {
          "children": [{
           "children": [{
            "children": [],
            "type": "3.9"
           }],
           "type": "python-version"
          }],
          "type": "with"
         }
        ],
        "type": "OBJ-NODE"
       },
       {
        "children": [
         {
          "children": [{
           "children": [],
           "type": "install dependencies"
          }],
          "type": "name"
         },
         {
          "children": [{
           "children": [],
           "type": "pip install -r requirements.txt"
          }],
          "type": "run"
         }
        ],
        "type": "OBJ-NODE"
       },
       {
        "children": [
         {
          "children": [{
           "children": [],
           "type": "build html documentaiton"
          }],
          "type": "name"
         },
         {
          "children": [{
           "children": [],
           "type": "make html"
          }],
          "type": "run"
         }
        ],
        "type": "OBJ-NODE"
       },
       {
        "children": [
         {
          "children": [{
           "children": [],
           "type": "install tinytex"
          }],
          "type": "name"
         },
         {
          "children": [{
           "children": [],
           "type": "r-lib/actions/setup-tinytex@version"
          }],
          "type": "uses"
         }
        ],
        "type": "OBJ-NODE"
       },
       {
        "children": [
         {
          "children": [{
           "children": [],
           "type": "install latex packages"
          }],
          "type": "name"
         },
         {
          "children": [{
           "children": [],
           "type": "tlmgr install tabulary latexmk ulem environ trimspaces titlesec \\\\\\n      varwidth framed threeparttable wrapfig upquote capt-of \\\\\\n      multirow eqparbox needspace fncychap letltxmacro\\n"
          }],
          "type": "run"
         }
        ],
        "type": "OBJ-NODE"
       },
       {
        "children": [
         {
          "children": [{
           "children": [],
           "type": "build pdf documentaiton"
          }],
          "type": "name"
         },
         {
          "children": [{
           "children": [],
           "type": "make latexpdf\\ncp build/latex/makefile.pdf build/html/\\n"
          }],
          "type": "run"
         }
        ],
        "type": "OBJ-NODE"
       },
       {
        "children": [
         {
          "children": [{
           "children": [],
           "type": "deploy documentation"
          }],
          "type": "name"
         },
         {
          "children": [{
           "children": [],
           "type": "peaceiris/actions-gh-pages@version"
          }],
          "type": "uses"
         },
         {
          "children": [{
           "children": [],
           "type": "${{ github.ref == 'refs/heads/main' }}"
          }],
          "type": "if"
         },
         {
          "children": [
           {
            "children": [{
             "children": [],
             "type": "${{ secrets.github_token }}"
            }],
            "type": "github_token"
           },
           {
            "children": [{
             "children": [],
             "type": "./build/html"
            }],
            "type": "publish_dir"
           },
           {
            "children": [{
             "children": [],
             "type": "TRUE"
            }],
            "type": "force_orphan"
           },
           {
            "children": [{
             "children": [],
             "type": "github-actions[bot]"
            }],
            "type": "user_name"
           },
           {
            "children": [{
             "children": [],
             "type": "github-actions[bot]@users.PATH-OR-URL"
            }],
            "type": "user_email"
           }
          ],
          "type": "with"
         }
        ],
        "type": "OBJ-NODE"
       }
      ],
      "type": "steps"
     }
    ],
    "type": "deploy"
   }],
   "type": "build"
  }
 ],
 "type": "github-file"
}